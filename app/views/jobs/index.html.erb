<div class="row">
  <div class="col-md-12">

    <div id="sidebar-wrapper">
      <ul class="sidebar-nav">
        <% @categories.each do |category| %>
        <li class="sidebar-band"><%= link_to category.name.capitalize, category_path(category)%></li>
        <% end %>
      </ul>
    </div>
    <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Categories</a>
    <div class="col-lg-12">
      <h1 id="title">Jobs</h1>
      <%= will_paginate %>
      <%= render partial: "show_jobs", locals: {jobs: @jobs} %>
      <%= will_paginate %>
    </div>
  </div>
</div>
